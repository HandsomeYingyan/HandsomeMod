+++ b/arch/arm/boot/dts/sun8i-v3s-licheepi-zero.dts
@@ -91,6 +91,16 @@
     status = "okay";
 };
 
+&i2c0 {
+	status = "okay";
+
+	ns2009: ns2009@48 {
+		compatible = "nsiway,ns2009";
+		reg = <0x48>;
+	};
+};
+
+
 &usbphy {
     usb0_id_det-gpios = <&pio 5 6 GPIO_ACTIVE_HIGH>; /* USB_ID PF6 */
     status = "okay";
